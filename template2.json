{
  "version": "0.0.1",
  "name": "cnty",
  "auth": {
    "method": "open"
  },
  "placeholders": {
    "color": {
      "type": "css_color",
      "default": "#FF6699"
    }
  },
  "layergroup": {
    "version": "1.0.1",
    "layers": [
      {
        "type": "cartodb",
        "options": {
          "cartocss_version": "2.3.0",
          "cartocss": "#layer { polygon-fill: <%= color %>; polygon-opacity: 0.5; line-color: #FFF; line-width: 1; line-opacity: 1; }",
          "sql": "select * from cnty where area_code like '<%= area_code %>%'"
        }
      }
    ]
  },
  "view": {
    "zoom": 6,
    "center": {
      "lng": -83,
      "lat": 33
    }
  }
}